def unique_elements(nums):
    unique_list = list(set(nums))  # Convert set back to list to preserve order (as set does not maintain order)
    return unique_list

# Test Cases
test_case_1 = [3, 7, 3, 5, 2, 5, 9, 2]
test_case_2 = [-1, 2, -1, 3, 2, -2]
test_case_3 = [1000000, 999999, 1000000]

print(unique_elements(test_case_1))  
print(unique_elements(test_case_2))  
print(unique_elements(test_case_3))  
INPUT OUTPUT:
[3,7,5,2,9]
[-1,2,3,-2]
[1000000,999999]
